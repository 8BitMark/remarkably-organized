// Place your settings in this file to overwrite default and user settings.
{
  "typescript.check.tscVersion": false,
  "files.exclude": {
    "node_modules/": false,
    "dist/": false,
    "lib/": false,
    ".tmp/": false,
    "platforms/": false,
    "www/": false,
    "plugins/": false,
    "resources/": false,
    ".editorconfig": false,
    ".gitignore": false,
    "ionic.config.json": false,
    "tsconfig.json": false,
    "tslint.json": false,
    ".vscode/": false,
    "hooks/": false,
    ".sourcemaps/": false,
    "cordova": false,
    "README.md": false,
    ".firebaserc": false,
    "package-lock.json": false,
    "**/manifest.json": false,
    "**/tsconfig.*.json": false,
    "**/polyfills.ts": false,
    "**/main.ts": false,
    "src/environments": false,
    "src/assets": false,
    "**/jest.config.js": false,
    "**/jest.ts": false,
    "**/gulpfile.js": false,
    "deploy-firebase-functions.js": false,
    "cors.json": false,
    ".firebase": false,
    "browserslist": false,
    "deploy-staging-code.js": false,
    "fontello*": false,
    "src/typings.d.ts": false,
    "**/main.server.ts": false,
    "scripts": false,
    ".eslintrc.cjs": false,
    ".prettierrc": false,
    "**/manifest.webmanifest": false,
    "**/.svelte-kit": false,
    "**/.env": false,
    "**/tsconfig.json": false,
    "**/svelte.config.js": false,
    "**/*d.ts": false,
    "**/node_modules": false,
    "**/.*": false,
    "apps/app/adapter": false,
    "apps/app/static": false,
    "apps/app/app.html": false,
    "**/*.config.ts": false,
    "pnpm-lock.yaml": false,
    "pnpm-workspace.yaml": false,
    "turbo.json": false,
    "**/dist": false
  },
  "typescript.tsserver.log": "off",
  "cssvar.files": [
    "**/*.css",
    "/src/lib/styles/global.scss",
    "**/*.scss"
  ],
  "workbench.editor.labelFormat": "short",
  "explorer.sortOrder": "default",
  "svelte.plugin.svelte.compilerWarnings": {
    "unknownAtRules": "ignore"
  },
  "vitest.commandLine": "pnpm run test --",
  "vitest.enable": true,
  "svelte.plugin.svelte.defaultScriptLanguage": "ts"
}
